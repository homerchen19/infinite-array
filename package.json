{
  "name": "infinite-array",
  "description": "Infinite array index using ES2015 Proxy",
  "author": "xxhomey19",
  "license": "MIT",
  "repository": "xxhomey19/infinite-array",
  "version": "1.0.2",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && babel index.js -d lib --ignore '**/*.spec.js'",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_ENV=test jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "jest --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "prettier-package-json": "^1.4.0",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "array",
    "es2015",
    "index",
    "indice",
    "infinite",
    "proxies",
    "proxy",
    "reverse"
  ],
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
