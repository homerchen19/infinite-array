{
  "name": "infinite-array",
  "description": "Infinite array index using ES2015 Proxy",
  "author": "xxhomey19",
  "license": "MIT",
  "repository": "xxhomey19/infinite-array",
  "version": "1.1.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "lint": "tslint index.ts",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_ENV=test jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "prettier-package-json": "^1.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.7.1"
  },
  "keywords": [
    "array",
    "es2015",
    "index",
    "indice",
    "infinite",
    "proxies",
    "proxy",
    "reverse"
  ],
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "types": "lib/index.d.js"
}
